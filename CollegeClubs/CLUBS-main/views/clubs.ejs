  <style>
    /* The grid container */
.club-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  grid-gap: 20px;
  padding: 20px;
}
.club-container {
  background-color: #f1f1f1;
 margin-top: 40px;
  padding: 20px;
  border-radius: 5px;
  text-align: center;
  transition: all 0.3s ease-in-out;
}

.club-container:hover {
  transform: translateY(-5px);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
}

.prayatnam-container {
  animation: pulse 2s infinite alternate;
}

.prayatnam-link {
  position: relative;
  display: inline-block;
  padding: 10px 20px;
  /* border: 1px solid #000; */
  text-decoration: none;
  color:#502c4c;
}
.prayatnam-link:hover {
  animation: moveUpDown 0.5s linear infinite;
}
@keyframes moveUpDown {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}



.chitram-container {
  animation: pulse 1s 0.2s infinite alternate;
}

.music-container {
  animation: pulse 1s 0.4s infinite alternate;
}

.physical-education-container {
  animation: pulse 1s 0.6s infinite alternate;
}

.brindavanam-assocaition-container {
  animation: pulse 1s 0.8s infinite alternate;
}

/* The pulse animation */
@keyframes pulse {
  from { transform: scale3d(1, 1, 1); }
  to { transform: scale3d(1.1, 1.1, 1.1); }
}

