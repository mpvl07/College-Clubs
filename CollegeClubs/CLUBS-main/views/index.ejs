       .card {
  width: 300px;
  height: 300px;
  margin: 0 auto;
  position: relative;
  top:128px;
  overflow: hidden;
}
.card .card-body {
    padding-top:30px;
  position: absolute;
  animation: scroll-up 5s ease-in infinite;
}

.card .event-name {
  margin: 0;
}

@keyframes scroll-up {
  0% {
    transform: translateY(0%);
  }
  100% {
    transform: translateY(-100%);
  }
}
 

